{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "globalSetup": "./test/setup.ts",
  "globalTeardown": "./test/teardown.ts",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "coveragePathIgnorePatterns": [
    "node_modules",
    "src/examples",
    "src/utils/seed/index.ts",
    "test/seed/*",
    "src/utils/seed/*",
    "src/utils/health/*"
  ]
}
